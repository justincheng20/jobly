{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "_handle",
    "_name"
  ],
  "properties": {
    "_handle": {
      "$id": "#/properties/handle",
      "type": "string",
      "title": "The Handle Schema",
      "default": "",
      "examples": [
        "dot-net"
      ],
      "pattern": "^(.*)$"
    },
    "_name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "also the internet"
      ],
      "pattern": "^(.*)$"
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
      "type": "integer",
      "title": "The Num_employees Schema",
      "default": 0,
      "examples": [
        20
      ]
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "it's an internet but networks"
      ],
      "pattern": "^(.*)$"
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "title": "The Logo_url Schema",
      "default": "",
      "examples": [
        "www.com"
      ],
      "pattern": "^(.*)$"
    }
  }
}