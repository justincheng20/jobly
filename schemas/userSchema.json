{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "username",
      "password",
      "first_name",
      "last_name",
      "email"
    ],
    "properties": {
      "username": {
        "$id": "#/properties/username",
        "type": "string",
        "title": "The Username Schema",
        "default": "",
        "examples": [
          "testusertoo"
        ],
        "pattern": "^(.*)$"
      },
      "password": {
        "$id": "#/properties/password",
        "type": "string",
        "title": "The Password Schema",
        "default": "",
        "examples": [
          "this_is we didn't forget! to hash this"
        ],
        "pattern": "^(.*)$"
      },
      "first_name": {
        "$id": "#/properties/first_name",
        "type": "string",
        "title": "The First_name Schema",
        "default": "",
        "examples": [
          "person"
        ],
        "pattern": "^(.*)$"
      },
      "last_name": {
        "$id": "#/properties/last_name",
        "type": "string",
        "title": "The Last_name Schema",
        "default": "",
        "examples": [
          "personson"
        ],
        "pattern": "^(.*)$"
      },
      "email": {
        "$id": "#/properties/email",
        "type": "string",
        "title": "The Email Schema",
        "default": "",
        "examples": [
          "@place"
        ],
        "pattern": "^(.*)$"
      },
      "photo_url": {
        "$id": "#/properties/photo_url",
        "type": "string",
        "title": "The Photo_url Schema",
        "default": "",
        "examples": [
          ".com"
        ],
        "pattern": "^(.*)$"
      }
    },
    "additionalProperties": false
  }