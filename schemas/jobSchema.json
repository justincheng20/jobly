{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "title",
      "salary",
      "equity",
      "company_handle"
    ],
    "properties": {
      "title": {
        "$id": "#/properties/title",
        "type": "string",
        "title": "The Title Schema",
        "default": "",
        "examples": [
          "the most workers"
        ],
        "pattern": "^(.*)$"
      },
      "salary": {
        "$id": "#/properties/salary",
        "type": "number",
        "title": "The Salary Schema",
        "default": 0.0,
        "examples": [
          1.0
        ],
        "minimum":0
      },
      "equity": {
        "$id": "#/properties/equity",
        "type": "number",
        "title": "The Equity Schema",
        "default": 0.0,
        "examples": [
          0.0
        ],
        "minimum":0,
        "maximum":1
      },
      "company_handle": {
        "$id": "#/properties/company_handle",
        "type": "string",
        "title": "The Company_handle Schema",
        "default": "",
        "examples": [
          "test"
        ],
        "pattern": "^(.*)$"
      }
    }
  }